    "gatsby-plugin-sharp": "^2.0.34",
    "gatsby-source-contentful": "^2.0.48",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.3.0",
    "lodash": "^4.17.11",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.0",
    "sharp": "^0.23.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "dotenv": "^8.0.0",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.12.4",
    "gatsby": "^2.17.4",
    "http-server": "^0.11.1",
    "prettier": "^1.17.0"
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
