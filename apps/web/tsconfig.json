{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    // "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve",
    "lib": ["dom", "es2015", "es2017"],
    "noEmit": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": [
    "./src/**/*",
    "./types/**/*",
    "./src/gatsby-types.d.ts",
    "./gatsby-*.tsx",
    "./gatsby/**/*.ts"
  ]
}
